<?xml version="1.0" encoding="utf-8"?>
<Containers>

  <container id="wood_chest">
    <sprite name="wood_chest" />
    <animations>
      <animation run="wood_chest_open" while="open" />
      <animation run="wood_chest_close" while="close" />
    </animations>
  </container>

  <container id="iron_chest">
    <sprite name="iron_chest" />
    <animations>
      <animation run="iron_chest_open" while="open" />
      <animation run="iron_chest_close" while="close" />
    </animations>
  </container>
  
</Containers>